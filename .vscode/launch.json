{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch examples/blog",
			"preLaunchTask": "compile-konstellio-server",
			"program": "${workspaceFolder}/packages/examples/blog/index.js"
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Launch vscode-fs-provider",
			"preLaunchTask": "watch-konstellio-vscode-fs-provider",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/packages/tools/vscode-fs-provider"
			],
			"outFiles": [
				"${workspaceFolder}/packages/tools/vscode-fs-provider/dist/**/*.js"
			]
		},
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "Launch vscode-fs-provider Test",
			"preLaunchTask": "watch-konstellio-vscode-fs-provider",
			"runtimeExecutable": "${execPath}",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/packages/tools/vscode-fs-provider",
				"--extensionTestsPath=${workspaceFolder}/packages/tools/vscode-fs-provider/dist/test"
			],
			"outFiles": [
				"${workspaceFolder}/packages/tools/vscode-fs-provider/dist/test/**/*.js"
			]
		}
	]
}