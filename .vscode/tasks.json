	"version": "2.0.0",
	"tasks": [
		{
			"label": "Compile server",
			"type": "npm",
			"script": "compile",
			"path": "packages/konstellio/server/",
			"problemMatcher": [
				{
					"base": "$tsc",
					"fileLocation": [
						"relative",
						"${workspaceFolder}/packages/konstellio/server/"
					]
				}
			]
		}
	]
}