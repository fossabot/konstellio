{
    "name": "konstellio-fs-provider",
    "displayName": "Konstellio FS Provider",
    "description": "",
    "version": "0.0.3",
    "publisher": "konstellio",
    "engines": {
        "vscode": "^1.23.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onFileSystem:ftp",
        "onFileSystem:ftps",
        "onFileSystem:sftp",
        "konstelliofs:downloadSelected",
        "konstelliofs:uploadHere"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "konstelliofs.downloadSelected",
                "title": "Download"
            },
            {
                "command": "konstelliofs.uploadFilesHere",
                "title": "Upload files here"
            },
            {
                "command": "konstelliofs.uploadFoldersHere",
                "title": "Upload folders here"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "konstelliofs.downloadSelected",
                    "when": "resourceScheme =~ /^ftp$|^ftps$|^sftp$/"
                },
                {
                    "command": "konstelliofs.uploadFilesHere",
                    "when": "resourceScheme =~ /^ftp$|^ftps$|^sftp$/"
                },
                {
                    "command": "konstelliofs.uploadFoldersHere",
                    "when": "resourceScheme =~ /^ftp$|^ftps$|^sftp$/"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/ftp": "^0.3.29",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/ssh2": "^0.5.35",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "@konstellio/disposable": "0.0.12",
        "@konstellio/fs": "0.0.7",
        "@konstellio/promised": "0.0.3",
        "ftp": "^0.3.10",
        "ssh2": "^0.6.1"
    }
}
