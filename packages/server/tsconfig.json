{
	"compilerOptions": {
		"alwaysStrict": true,
		"declaration": false,
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "./dist",
		"rootDir": "./src",
		"sourceMap": true,
		"strictNullChecks": true,
		"target": "es6",
		"jsx": "react",
		"allowSyntheticDefaultImports": true
	},
	"exclude": [
		"node_modules"
	]
}